{
  "name": "slideshow",
  "author": "Trystan Eveno",
  "licence": "MIT",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "test": "npm run lint",
    "install": "npm run build:dev",
    "build:stats": "webpack --profile --json > webpack-stats.json",
    "build:watch": "webpack --watch",
    "build:dev": "webpack",
    "build": "NODE_ENV=production webpack -p",
    "deploy": "npm run build && git add -A && git commit -nm ':rocket: deploy' && git push && git subtree push --prefix web origin gh-pages",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "jquery-mousewheel": "^3.1.13",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browserslist": "^2.9.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.12.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-react": "^7.5.1",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.8.2",
    "webpack-monitor": "^1.0.14"
  }
}
